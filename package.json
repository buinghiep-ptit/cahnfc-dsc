{
  "name": "nextjs-typescript-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "lint": "next lint",
    "lint:check": "eslint \"src/**/*\" ./pages/** -c .eslintrc.json --ext .ts,.tsx",
    "lint:fix": "eslint \"src/**/*\" ./pages/** -c .eslintrc.json --fix --ext .ts,.tsx",
    "format:check": "prettier --check \"{,!(.next)/**/}*.{ts, tsx}\"",
    "format:fix": "prettier --write \"{,!(.next)/**/}*.{ts, tsx}\"",
    "prepare": "husky install",
    "postinstall": "husky install",
    "test-all": "npm run format:check && npm run lint:check && npm run type-check"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn format:fix",
      "yarn lint:fix",
      "git add"
    ]
  },
  "dependencies": {
    "@emotion/cache": "^11.10.0",
    "@emotion/react": "^11.10.0",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.9.2",
    "@mui/system": "^5.9.2",
    "@tanstack/react-query": "^4.0.10",
    "@tanstack/react-query-devtools": "^4.0.10",
    "axios": "^0.27.2",
    "camelize": "^1.0.0",
    "cookies": "^0.8.0",
    "http-proxy": "^1.18.1",
    "lodash": "^4.17.21",
    "next": "12.2.3",
    "next-auth": "^4.10.3",
    "query-string": "^7.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.34.0",
    "tailwindcss": "^3.1.8",
    "yup": "^0.32.11",
    "zustand": "^4.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@next/eslint-plugin-next": "^12.2.3",
    "@types/axios": "^0.14.0",
    "@types/cookies": "^0.7.7",
    "@types/http-proxy": "^1.17.9",
    "@types/next": "^9.0.0",
    "@types/node": "18.6.3",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "8.21.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "12.2.3",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.3.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unicorn": "^43.0.2",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "typescript": "4.7.4"
  }
}
